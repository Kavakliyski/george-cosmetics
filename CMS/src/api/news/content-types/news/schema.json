{
  "kind": "collectionType",
  "collectionName": "newsposts",
  "info": {
    "singularName": "news",
    "pluralName": "newsposts",
    "displayName": "news"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "news_title": {
      "type": "string",
      "required": true
    },
    "news_body": {
      "type": "richtext",
      "required": true
    },
    "news_picture": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true,
      "required": false
    },
    "news_description": {
      "type": "string",
      "required": true
    },
    "news_date": {
      "type": "date"
    }
  }
}
